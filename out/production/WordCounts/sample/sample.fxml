<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.TextField?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <Label GridPane.rowIndex="1" text="Please enter URL to find top 20 words:" />
    <TextField GridPane.rowIndex="2" maxHeight="2" fx:id="urlInput"/>
    <Button GridPane.rowIndex="3" text="Find Top 20 Words" onAction="#findTop20"/>
    <Label GridPane.rowIndex="4" fx:id="listTop20"/>

</GridPane>